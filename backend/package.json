{
  "name": "diamondbridge-backend",
  "version": "1.0.0",
  "description": "Backend API для DiamondBridge - интеграция AI сервисов, медиа обработки, Google Drive и уведомлений",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "logs": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "logs:api": "tail -f logs/api.log",
    "clean:logs": "rm -rf logs/*"
  },
  "keywords": [
    "api",
    "ai",
    "openai",
    "anthropic",
    "google-ai",
    "media-processing",
    "google-drive",
    "notifications",
    "express",
    "nodejs"
  ],
  "author": "DiamondBridge Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "mongoose": "^8.0.3",
    "redis": "^4.6.10",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "fluent-ffmpeg": "^2.1.2",
    "tesseract.js": "^4.1.1",
    "openai": "^4.24.1",
    "@anthropic-ai/sdk": "^0.8.1",
    "@google/generative-ai": "^0.2.1",
    "@google-cloud/speech": "^6.0.1",
    "@google-cloud/text-to-speech": "^5.0.1",
    "googleapis": "^129.0.0",
    "nodemailer": "^6.9.7",
    "web-push": "^3.6.6",
    "twilio": "^4.19.3",
    "firebase-admin": "^11.11.1",
    "socket.io": "^4.7.4",
    "cron": "^3.1.6",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
