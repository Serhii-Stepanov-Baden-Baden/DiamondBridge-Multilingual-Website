  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diamondbridge/backend.git"
  },
  "bugs": {
    "url": "https://github.com/diamondbridge/backend/issues"
