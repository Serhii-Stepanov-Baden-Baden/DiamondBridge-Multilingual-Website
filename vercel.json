{
  "version": 2,
  "name": "diamondbridge-app",
  "builds": [
    {
      "src": "diamondbridge-multilingual/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/diamondbridge-multilingual/dist/$1"
    }
  ],
  "env": {"NODE_ENV": "production"},
  "regions": ["fra1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {"key": "X-Content-Type-Options", "value": "nosniff"},
        {"key": "X-Frame-Options", "value": "DENY"},
        {"key": "X-XSS-Protection", "value": "1; mode=block"}
      ]
    }
  ]
}
