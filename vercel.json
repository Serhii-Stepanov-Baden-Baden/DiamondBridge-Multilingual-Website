{
  "builds": [
    {
      "src": "diamondbridge-multilingual/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "diamondbridge-multilingual/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "diamondbridge-multilingual/dist/index.html"
    }
  ]
}
